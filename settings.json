{
    "workbench.layoutControl.type": "toggles",
    "git.autofetch": true,
    "workbench.iconTheme": "material-icon-theme",
    "vscode-pets.petSize": "medium",
    "vscode-pets.petType": "totoro",
    "vscode-pets.throwBallWithMouse": true,
    "git.openRepositoryInParentFolders": "always",
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "kitty",
    "debug.terminal.clearBeforeReusing": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.initialHint": false,
    "terminal.integrated.tabs.defaultColor": "terminal.ansiBlack",
    "terminal.integrated.defaultProfile.linux": "bash",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "notebook.defaultFormatter": "PKief.material-icon-theme",
    "editor.defaultFormatter": "PKief.material-icon-theme",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "zenMode.restore": false,
    "workbench.productIconTheme": "adwaita",
    "window.autoDetectHighContrast": false,
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.location": "bottom",
    "window.customTitleBarVisibility": "auto",
    "workbench.tree.enableStickyScroll": false,
    "zenMode.hideActivityBar": false,
    "window.titleBarStyle": "custom",
    "cSpell.userWords": [
        "fileupload",
        "totoro"
    ],
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "sema",
